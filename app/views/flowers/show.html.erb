<h1>Flowervase</h1>
<h3>Tweet詳細</h3>
<div class="flower">
<%= image_tag @flower.resize_image_top if @flower.image_top.attached? %>
  <p><%= @flower.shop %></p>
  
  <p><%= @flower.prefecture %></p>
  <p><%= @flower.station %></p>
  <p><%= @flower.comment %></p>
  <p><%= @flower.created_at %></p>

  <%= link_to "編集する", edit_flower_path(@flower.id) %>
</div>
<%= link_to "Tweet一覧に戻る", flowers_path %>
